#include<iostream>

int functie_ex_1()
{ int alta_variabila=0; 
	for(int i=0; i<=100; i++)
	{
		if((i<10 || i>20) && (i%2 == 0) ) alta_variabila += i;
		if(i<=10) std::cout<<i<<" ";
	}
return alta_variabila;
}

void functie_ex_2()
{
	for(int i=1; i<=100; i++)
	{
		if(i%3 == 0) std::cout<<"Fizz";
		if(i%5 == 0) std::cout<<"Buzz";
    if(i%3 !=0 && i%5!=0) std::cout<<i;
		std::cout<<std::endl;
	}
}

int main()
{
	functie_ex_1();
	functie_ex_2();
	return 0;
}
